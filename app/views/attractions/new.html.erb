<%=form_for Attraction.new do |f|%>
  <%=label :name%><br>
  <%=f.text_field :name%><br>

  <%=label :tickets, "Ticket Cost"%><br>
  <%=f.number_field :tickets%><br>

  <%=label :nausea_rating%><br>
  <%=f.number_field :nausea_rating%><br>

  <%=label :happiness_rating%><br>
  <%=f.number_field :happiness_rating%><br>

  <%=label :min_height%><br>
  <%=f.number_field :min_height%><br>

  <%=f.submit%>
<%end%>
